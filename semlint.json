{
  "backend": "cursor-cli",
  "budgets": {
    "timeout_ms": 120000
  },
  "output": {
    "format": "text"
  },
  "execution": {
    "batch": false
  },
  "security": {
    "secret_guard": true,
    "allow_patterns": [],
    "ignore_files": [".gitignore", ".cursorignore", ".semlintignore"]
  },
  "rules": {
    "disable": [],
    "severity_overrides": {
      "SEMLINT_NAMING_001": "warn",
      "SEMLINT_PATTERN_002": "warn",
      "SEMLINT_SWE_003": "warn"
    }
  },
  "backends": {
    "cursor-cli": {
      "executable": "cursor",
      "model": "composer-1.5",
      "args": [
        "agent",
        "{prompt}",
        "--model",
        "{model}",
        "--print",
        "--mode",
        "ask",
        "--output-format",
        "text",
        "--trust"
      ]
    }
  }
}
